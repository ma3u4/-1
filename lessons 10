package com.company;

import java.util.Scanner;
public class summa {public static void main(String[] args){
    Scanner s = new Scanner(System.in);
    System.out.println("Введите число1");
    int x = s.nextInt();
    System.out.println("Введите число2");
    int z = s.nextInt();
    int y = x+z;
    System.out.println("Сумма чисел "+y);
}
}
