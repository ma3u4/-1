package com.company;
import java.util.Scanner;
public class modul {
    public static void main(String[] args ){
        Scanner s = new Scanner(System.in);
        System.out.println("Введите число");
        int x = s.nextInt();

        if(x <0) {
            System.out.print(-(x));
        }else
            System.out.println(x);
    }

}
